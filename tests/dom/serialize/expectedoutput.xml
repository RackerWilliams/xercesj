<!--add p:foo, uri=emptyStr, xmlns:p=emptyStr-->
<foo xmlns=""/>

<!--add s:foo, uri=emptyStr, xmlns:p=emptyStr-->
<foo xmlns=""/>

<!--create element: with 2 xmlns-->
<elem xmlns="namespace1"/>

