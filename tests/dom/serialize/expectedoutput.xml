  <!-- both first and second "elem" elements have 
       xmlns="" declaration      
   -->
   <empty xmlns="">
	<elem xmlns=""/>
	<elem xmlns="newURI"><a/></elem>
   </empty>


   <!-- the second element "good" had declaration 
        xmlns:n1="http://www.w3.org" -->
