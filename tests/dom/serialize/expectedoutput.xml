  	<good a="1" b="2"/>
  	<good a="1" n1:a="2"/>
   </x>
<!--add element in the same scope-->
<s:bar xmlns:s="urn:schemas-xmlsoap-org:soap.v1"/>

<!--add element with s bound to different namespace-->
<s:hello xmlns:s="s_bound_to_this"/>

<!--add element with no prefix bound to different scope than default prefix-->
<emptyNS xmlns="empty_rebound"/>

<!--add element no prefix no namespace-->
<defaultNamespace xmlns=""/>

<!--add element with prefix s and empty namespace-->
<noNamespace xmlns=""/>

<!--create element: _attr_ bound to _hi_, local declaration of xmlns:attr = boo-->
